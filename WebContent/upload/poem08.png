               // console.log("PCVer:"+"https://blog.aladin.co.kr/api/product.aspx?output=jsonp&method=" +method +"&itemid=268630213&readStatus="  + readStatus );

                $j.ajax({
                    url: "https://blog.aladin.co.kr/api/product.aspx?output=jsonp",
                    data: { method: method, itemid: 268630213, readStatus: readStatus },
                    dataType: "jsonp",
