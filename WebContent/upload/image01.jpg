    function Basket_Add_All_EbookSeries(){
        var checkedList = $j("#p_connect");
        var isbn = CheckIsbnList(checkedList)
        //구매 탭을 클릭했을때 값 설정
        isRent = $j(".ebookRentTitle").attr("class") == "ebookRentTitle on" ? 1 : 0;

        //대여일때 3
        uiType = isRent ? 3 : 1;

        if (uiType == 2) {
            SafeBasket_ListAddByAjax(isbn, null, { left: -8, top: 25 });
        }
        else if (uiType == 3) {
            SafeBasket_RentListAddByAjax2(isbn, null, { left: 0, top: 30 });
        }
        else {
            SafeBasket_ListAddByAjax2(isbn, null, { left: 0, top: 30 });
        }
    }

    //공용모듈에서 alert 제거
    function CheckIsbnList(o)
    {
        // o의 child중, 체크된 상품의 ISBN리스트 가져오기
        var checkedList = $j(o).find("input:checked");
        var isbnCol = '';
        checkedList.each(function (index) {
            var n = $j(this).attr('name');

            if (n.indexOf('.') > -1) {
                var isbn = n.substring(n.indexOf('.') + 1, n.length);

                if (isbnCol == '')
                    isbnCol = isbn;
                else
                    isbnCol += ',' + isbn;
            }
        });
        return isbnCol;
    }

