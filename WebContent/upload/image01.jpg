        var divAddBasket = $("#addSeries");
        var htmResult = $("#addSeriesResult");
        var query = 'method=quickbuylist&isprebasketkeep=0&isbn=' + pkList + '&isrent=' + IsRent;

        $j.ajax({
            type: "POST",
            url: "/shop/BasketAjax.aspx",
            data: query,
            cache: false,
            dataType: "json",
            timeout: 3000,
            success: function (json) {
