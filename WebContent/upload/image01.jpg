    function ChoiceItemInfo(elm) {
        
        var root = elm.closest(".divGroupBook");
        var frmCheckBox = root.find(".cbSeries");
        var priceSales = 0;
        var rentPriceSales = 0;
        var checkCnt = 0;
        var isRent = $j(".ebookRentTitle").attr("class") == "ebookRentTitle on" ? 1 : 0;
        var isEbookSeries = 0;
        var hasCouponPrice = 0;

